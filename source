local function TAIVLO_fake_script() -- ScreepstuHub.Close.LocalScript 
	local script = Instance.new('LocalScript', ScreepstuHub.Close)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Container1.Visible = false
		script.Parent.Parent.Parent.Container2.Visible = false
		script.Parent.Parent.Parent.Container3.Visible = false
		script.Parent.Parent.Parent.Container4.Visible = false
		script.Parent.Parent.Parent.Container5.Visible = false

		wait(.5)

		script.Parent.Parent.Parent.Verti_Line:TweenPosition(
			UDim2.new(5, 100, 0 ,30),
			Enum.EasingDirection.InOut,
			Enum.EasingStyle.Quint,
			2
		)

		script.Parent.Parent.Parent.Tab:TweenPosition(
			UDim2.new(5, 0, 0 ,30),
			Enum.EasingDirection.InOut,
			Enum.EasingStyle.Quint,
			2
		)

		script.Parent.Parent.Parent.Horiz_Line:TweenPosition(
			UDim2.new(5, 0, 0 ,29),
			Enum.EasingDirection.InOut,
			Enum.EasingStyle.Quint,
			2
		)

		script.Parent.Parent.Parent.MainTop:TweenPosition(
			UDim2.new(5, 0, 0 ,0),
			Enum.EasingDirection.InOut,
			Enum.EasingStyle.Quint,
			2
		)

		task.wait(2)

		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(TAIVLO_fake_script)()
