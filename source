local function BQDCZ_fake_script() -- ScreepstuHub.FieldofView.LocalScript 
	local script = Instance.new('LocalScript', ScreepstuHub.FieldofView)

	local textBox = script.Parent

	function isNumber(str)
		if tonumber(str) ~= nil or str == 'inf' then
			return true
		end
	end

	local function onTextChanged()
		local text = textBox.Text
		local number = tonumber(text)

		if number then
			local fov = number or 70
			if isNumber(fov) then
				workspace.CurrentCamera.FieldOfView = fov
			end
		end
	end

	textBox.Changed:Connect(onTextChanged)
end
coroutine.wrap(BQDCZ_fake_script)()
local function STRPW_fake_script() -- ScreepstuHub.Freeze.LocalScript 
	local script = Instance.new('LocalScript', ScreepstuHub.Freeze)

	local enable = false

	script.Parent.MouseButton1Click:Connect(function()
		if enable == false then
			enable = true
			script.Parent.UIStroke.Color = Color3.fromRGB(43, 177, 255)
			script.Parent.Text = "is Freezing: Yes"

			task.spawn(function()
				for i, x in next, game.Players.LocalPlayer.Character:GetDescendants() do
					if x:IsA("BasePart") and not x.Anchored then
						x.Anchored = true
					end
				end
			end)
		else
			enable = false
			script.Parent.UIStroke.Color = Color3.fromRGB(38, 255, 156)
			script.Parent.Text = "is Freezing: No"


			task.spawn(function()
				for i, x in next, game.Players.LocalPlayer.Character:GetDescendants() do
					if x:IsA("BasePart") and x.Anchored then
						x.Anchored = false
					end
				end
			end)
		end	
	end)
end
