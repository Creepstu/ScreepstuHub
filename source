local dapatkan_Screepstu_Hub = game.CoreGui:FindFirstChild("ScreepstuHub")

if dapatkan_Screepstu_Hub then
	dapatkan_Screepstu_Hub:Destroy()
else
	warn("Misi selesai, Screepstu Hub tidak ditemui dalam Core GUI")

	local StarterGui = game.StarterGui

	local aa = Instance.new('Sound', game.ReplicatedFirst)
	aa.SoundId = "rbxassetid://8470808181"
	aa.Volume = 1
	aa:Play()

	StarterGui:SetCore("SendNotification", {
		Title = "Hification";
		Text = "Hello there!";
		Duration = aa.TimeLength;
	})

	aa.Ended:Connect(function()
		aa:Destroy()
	end)
end

